inal":{"line":18,"column":28},"generated":{"line":1,"column":1347}},{"source":"05-aim-game/script.js","name":"getAttribute","original":{"line":18,"column":35},"generated":{"line":1,"column":1354}},{"source":"05-aim-game/script.js","original":{"line":18,"column":48},"generated":{"line":1,"column":1367}},{"source":"05-aim-game/script.js","name":"screens","original":{"line":19,"column":8},"generated":{"line":1,"column":1381}},{"source":"05-aim-game/script.js","original":{"line":19,"column":16},"generated":{"line":1,"column":1383}},{"source":"05-aim-game/script.js","name":"classList","original":{"line":19,"column":19},"generated":{"line":1,"column":1386}},{"source":"05-aim-game/script.js","name":"add","original":{"line":19,"column":29},"generated":{"line":1,"column":1396}},{"source":"05-aim-game/script.js","original":{"line":19,"column":33},"generated":{"line":1,"column":1400}},{"source":"05-aim-game/script.js","name":"startGame","original":{"line":20,"column":8},"generated":{"line":1,"column":1406}},{"source":"05-aim-game/script.js","name":"board","original":{"line":24,"column":0},"generated":{"line":1,"column":1413}},{"source":"05-aim-game/script.js","name":"addEventListener","original":{"line":24,"column":6},"generated":{"line":1,"column":1415}},{"source":"05-aim-game/script.js","original":{"line":24,"column":23},"generated":{"line":1,"column":1432}},{"source":"05-aim-game/script.js","original":{"line":24,"column":32},"generated":{"line":1,"column":1440}},{"source":"05-aim-game/script.js","name":"event","original":{"line":24,"column":33},"generated":{"line":1,"column":1449}},{"source":"05-aim-game/script.js","name":"event","original":{"line":25,"column":8},"generated":{"line":1,"column":1452}},{"source":"05-aim-game/script.js","name":"target","original":{"line":25,"column":14},"generated":{"line":1,"column":1454}},{"source":"05-aim-game/script.js","name":"classList","original":{"line":25,"column":21},"generated":{"line":1,"column":1461}},{"source":"05-aim-game/script.js","name":"contains","original":{"line":25,"column":31},"generated":{"line":1,"column":1471}},{"source":"05-aim-game/script.js","original":{"line":25,"column":40},"generated":{"line":1,"column":1480}},{"source":"05-aim-game/script.js","name":"score","original":{"line":26,"column":8},"generated":{"line":1,"column":1492}},{"source":"05-aim-game/script.js","name":"event","original":{"line":27,"column":8},"generated":{"line":1,"column":1496}},{"source":"05-aim-game/script.js","name":"target","original":{"line":27,"column":14},"generated":{"line":1,"column":1498}},{"source":"05-aim-game/script.js","name":"remove","original":{"line":27,"column":21},"generated":{"line":1,"column":1505}},{"source":"05-aim-game/script.js","name":"createRandomCircle","original":{"line":28,"column":8},"generated":{"line":1,"column":1514}}],"sources":{"05-aim-game/script.js":"const startButton = document.getElementById('start');\r\nconst screens = document.querySelectorAll('.screen');\r\nconst timeList = document.getElementById('time-list');\r\nconst gameTime = document.getElementById('time');\r\nconst board = document.getElementById('board');\r\nconst COLORS = ['#ffffff', '#ffff00', '#ff4500', '#800000', '#7cfc00', '#00ffff', '#00008b', '#ff1493', '#8b008b', '#006400', '#7fffd4', '#000000', '#db7093', '#5f9ea0', '#8B4513'];\r\n\r\nlet time = 0;\r\nlet score = 0;\r\n\r\nstartButton.addEventListener('click', (event) => {\r\n    event.preventDefault();\r\n    screens[0].classList.add('up');\r\n});\r\n\r\ntimeList.addEventListener('click', (event) => {\r\n    if (event.target.classList.contains('time-btn')) {\r\n        time = Number(event.target.getAttribute('data-time'));\r\n        screens[1].classList.add('up');\r\n        startGame();\r\n    }\r\n});\r\n\r\nboard.addEventListener('click', (event) => {\r\n    if (event.target.classList.contains('circle')) {\r\n        score++;\r\n        event.target.remove();\r\n        createRandomCircle();\r\n    }\r\n});\r\n\r\nfunction startGame() {\r\n    setInterval(decreaseTime, 1000);\r\n    createRandomCircle();\r\n    setTime(time);\r\n    //Вызов функции для автоматической игры: getWin();\r\n}\r\n\r\nfunction decreaseTime() {\r\n    let currentTime = --time;\r\n    if (currentTime === 0) {\r\n        finishGame();\r\n    }\r\n\r\n    if (currentTime < 10) {\r\n        currentTime = `0${currentTime}`;\r\n    }\r\n\r\n    setTime(currentTime);\r\n}\r\n\r\nfunction setTime(value) {\r\n    gameTime.innerHTML = `00:${value}`;\r\n}\r\n\r\nfunction finishGame() {\r\n    gameTime.parentNode.classList.add('hide');\r\n    board.innerHTML = `<h1>Ваш счет: <span class=\"primary\">${score}</span></h1>`;\r\n}\r\n\r\nfunction createRandomCircle() {\r\n    const circle = document.createElement('div');\r\n    const sizeCircle = getRandomNumber(20, 80);\r\n    const colorCircle = getRandomColor();\r\n    const {width, height} = board.getBoundingClientRect();\r\n    const xCoordinate = getRandomNumber(0, width - sizeCircle);\r\n    const yCoordinate = getRandomNumber(0, height - sizeCircle);\r\n\r\n    circle.classList.add('circle');\r\n    circle.style.width = `${sizeCircle}px`;\r\n    circle.style.height = `${sizeCircle}px`;\r\n    circle.style.backgroundColor = `${colorCircle}`;\r\n    circle.style.top = `${yCoordinate}px`;\r\n    circle.style.left = `${xCoordinate}px`;\r\n    board.appendChild(circle);\r\n}\r\n\r\nfunction getRandomNumber(max, min) {\r\n    return Math.round(Math.random() * (max - min) + min);\r\n}\r\n\r\nfunction getRandomColor() {\r\n    return COLORS[Math.floor(Math.random() * COLORS.length)];\r\n}\r\n\r\n// Функция для автоматической игры\r\n\r\nfunction getWin() {\r\n    function kill() {\r\n        const circle = document.querySelector('.circle');\r\n        if (circle) {\r\n            circle.click();\r\n        }\r\n    }\r\n\r\n    setInterval(kill, 70);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f093989383e91d6582be7791498a729a","cacheData":{"env":{}}}